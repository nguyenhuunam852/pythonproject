    "version": "0.2.0",
    "configurations": [
    
        {
            "name": "Python: Django",
            "type": "python",
            "request": "launch",
            "program": "I:/djangoWeb/pythonproject/manage.py",
            "args": [
                "runserver",
                "--noreload"
            ],
            "django": true
        },
        {
            "name": "Python: Celery",
            "type": "python",
            "request": "launch",
            "module": "celery",
            "console": "integratedTerminal",
            "args": [
                "-A",
                "tsbc",
                "worker",
                "-l",
                "info",
                "-P",
                "solo",
            ]
        }
    ]
}